type SignatureReply = record {
  r: text;
  s: text;
  v: nat8;
};

service : {
  "get_eth_brl_exchange": () -> (text);
  "public_key": () -> (variant { Ok: record { public_key_hex: text; }; Err: text });
  "signa": (text) -> (variant { Ok: SignatureReply; Err: text });
  "verify": (text, text, text, text) -> (variant { Ok: record { is_signature_valid: bool; }; Err: text });
}
